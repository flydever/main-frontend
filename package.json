{
  "name": "allio",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc",
    "build:ios-debug": "detox build --configuration ios.sim.debug",
    "build:ios-release": "detox build --configuration ios.sim.release",
    "build:android-debug": "detox build --configuration android.emu.debug",
    "build:android-release": "detox build --configuration android.emu.release",
    "test:ios": "detox test --configuration ios.sim.debug",
    "test:android": "detox test --configuration android.emu.debug",
    "e2e:ios": "npm run build:ios-debug && npm run test:ios",
    "e2e:android": "npm run build:android-debug && npm run test:android"
  },
  "dependencies": {
    "@aws-amplify/auth": "^4.3.18",
    "@aws-amplify/core": "^4.3.10",
    "@react-native-async-storage/async-storage": "^1.15.8",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/stack": "^6.0.7",
    "@reduxjs/toolkit": "^1.6.1",
    "@sentry/react-native": "^3.1.1",
    "@styled-system/theme-get": "^5.1.2",
    "@types/react-native-svg-charts": "^5.0.11",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/styled-components": "^5.1.15",
    "@types/styled-components-react-native": "^5.1.1",
    "@types/styled-system": "^5.1.13",
    "@types/styled-system__theme-get": "^5.0.1",
    "add": "^2.0.6",
    "amazon-cognito-identity-js": "^5.2.3",
    "appcenter": "4.2.0",
    "appcenter-analytics": "4.2.0",
    "appcenter-crashes": "4.2.0",
    "axios": "^0.21.4",
    "date-fns": "^2.27.0",
    "lodash": "^4.17.21",
    "react": "17.0.2",
    "react-native": "0.66.3",
    "react-native-dropdown-enhanced": "^1.1.4",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-pager-view": "^5.4.8",
    "react-native-persona": "^2.1.1",
    "react-native-plaid-link-sdk": "^7.2.0",
    "react-native-ratings": "^8.1.0",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-svg": "^12.1.1",
    "react-native-svg-charts": "^5.4.0",
    "react-native-swiper": "^1.6.0",
    "react-native-tab-view": "^3.1.1",
    "react-native-vector-icons": "^9.0.0",
    "react-redux": "^7.2.5",
    "styled-components": "^5.3.1",
    "styled-map": "^3.3.0",
    "styled-system": "^5.1.5",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.178",
    "@types/react-native": "^0.64.5",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^1.13.2",
    "detox": "^18.20.3",
    "eslint": "^7.14.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "jest-circus": "^27.2.0",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.4.1",
    "react-native-codegen": "^0.0.7",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
